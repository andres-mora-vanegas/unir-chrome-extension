{
  "manifest_version": 2,

  "name": "UCentral extension",
  "version": "0.1.0",
  "description": "Permite agregar nuevas funcionalidades a Blackboard LMS y OpenLMS",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "css": ["styles.css"],
      "js": ["jquery-3.2.1.min.js", "content.js"],
      "matches": ["https://ucentral.blackboard.com/*","http://elearning.ucentral.edu.co/*"]
    }
  ]
}
